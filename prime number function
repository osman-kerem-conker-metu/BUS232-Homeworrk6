lower = 2
upper = 71

#first 20 prime numbers are between 2 and 71.

print("Prime numbers between", lower, "and", upper, "are:")

for num in range(lower, upper + 1):
    #all prime numbers are greater than
    if num > 1:
        for i in range(2, num):
            if (num % i) == 0:
                break
            
            else:
                print(num)
